{
  "name": "dca",
  "engines": {
    "node": ">=10.6.0"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:sol && npm run lint:ts",
    "lint:fix": "npm run lint:js:fix && npm run lint:sol:fix && npm run lint:ts:fix",
    "lint:js": "eslint --ignore-path .gitignore .",
    "lint:js:fix": "eslint --ignore-path .gitignore . --fix",
    "lint:ts": "eslint . --ext .ts",
    "lint:ts:fix": "eslint . --ext .ts --fix",
    "lint:sol": "solhint 'contracts/**/*.sol' && prettier -c 'contracts/**/*.sol'",
    "lint:sol:fix": "prettier --write \"contracts/**/*.sol\"",
    "build": "yarn clean && yarn compile",
    "clean": "hardhat clean",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.5.1",
    "@ethersproject/solidity": "^5.7.0",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "3.1.2",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@openzeppelin/hardhat-upgrades": "^1.21.0",
    "@openzeppelin/test-helpers": "^0.5.16",
    "@primitivefi/hardhat-marmite": "^0.0.4",
    "@typechain/ethers-v5": "^10.1.1",
    "@typechain/hardhat": "^6.1.4",
    "@types/chai": "^4.3.3",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@uniswap/v2-core": "^1.0.1",
    "axios": "^1.1.3",
    "axios-retry": "^3.3.1",
    "chai": "^4.3.6",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eth-permit": "^0.2.3",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.7.2",
    "fs-extra": "^10.1.0",
    "hardhat": "^2.12.2",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.11.20",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.9",
    "node-fetch": "3.2.10",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "1.0.0-rc.1",
    "quickswap-sdk": "^3.0.14",
    "solhint": "^3.3.7",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.8.4"
  }
}
